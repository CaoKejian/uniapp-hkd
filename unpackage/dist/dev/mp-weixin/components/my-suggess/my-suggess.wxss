/* 水平间距 */
.wrapper {
  background-color: #fffae5;
  height: 100vh;
}
.item-search .item .item-list {
  width: 90%;
  margin: 0 auto;
  border: 1px solid #e7e7e7;
  background-color: #fff;
  padding: 8px 10px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.biglist-item .item {
  width: 100%;
  height: 100rpx;
  text-align: center;
  background-color: #f3f1ea;
  margin-bottom: 1px;
  line-height: 100rpx;
  color: #93918c;
}

