<view class="wrapper"><view class="map-instruction"><uni-section class="mb-10" vue-id="12f58e9e-1" title="地图展示" titleFontSize="18px" titleColor="#9E6D38" sub-title="民办博物馆 | 智慧楼 | 体育场···" type="line" bind:__l="__l"></uni-section><map style="width:100%;height:250px;" latitude="{{latitude}}" longitude="{{longitude}}" markers="{{covers}}" show-compass="true" enable-3D="true" data-event-opts="{{[['tap',[['openMap']]]]}}" bindtap="__e"></map></view><view class="video-instrusction"><uni-section class="mb-10" vue-id="12f58e9e-2" title="视频介绍" titleFontSize="18px" titleColor="#9E6D38" sub-title="学校 | 餐厅 | 快递···" type="line" bind:__l="__l"></uni-section><view class="video"><block wx:for="{{videoClass}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toVideoDetail',['$0'],[[['videoClass','',index,'_id']]]]]]]}}" class="video-item" bindtap="__e"><view class="container"><view class="video-item-left"><image src="{{item.img}}" mode></image></view><view class="video-item-right"><text class="title">{{item.title}}</text><text class="from">{{"来源："+item.from}}</text><text class="mount">{{"播放量："+item.view_coun}}</text><text class="mount">上传时间：<uni-dateformat vue-id="{{'12f58e9e-3-'+index}}" date="{{item.publishData}}" format="MM月dd hh:mm" threshold="{{[60000,3600000*24*30]}}" bind:__l="__l"></uni-dateformat></text></view></view></view></block></view></view><block wx:if="{{isShowWeb}}"><web-view src="{{url}}"></web-view></block></view>