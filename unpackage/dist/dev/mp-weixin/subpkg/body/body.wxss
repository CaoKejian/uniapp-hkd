/* 水平间距 */
.wrapper {
  width: 100%;
  height: 100vh;
  background-color: #fdfee4;
}
.wrapper .item {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.wrapper .item image {
  width: 100%;
}
.wrapper .item .item-list {
  padding-top: 50rpx;
  width: 100%;
  height: auto;
  background-color: #fff;
  flex: 1;
  text-align: center;
}
.wrapper .item .item-list image {
  width: 80%;
  height: 100rpx;
  margin-top: 50rpx;
}

